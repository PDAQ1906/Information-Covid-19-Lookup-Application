<Application x:Class="Server.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Server"
             StartupUri="MainWindow.xaml"
             ShutdownMode="OnMainWindowClose">
    <Application.Resources>
        <FontFamily x:Key="MDL2">pack://application:,,,/Fonts/#Segoe MDL2 Assets</FontFamily>
        <FontFamily x:Key="Password">pack://application:,,,/Fonts/#Password</FontFamily>
        <FontFamily x:Key="SegoeSemiBold">pack://application:,,,/Fonts/#Segoe UI Variable Static Display Semibold</FontFamily>
        <FontFamily x:Key="SegoeVarDisplay">pack://application:,,,/Fonts/#Segoe UI Variable Static Display</FontFamily>
        <FontFamily x:Key="SegoeVarBold">pack://application:,,,/Fonts/#Segoe Variable Bold</FontFamily>
    </Application.Resources>
</Application>
